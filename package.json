{
  "name": "argus-eyes",
  "version": "0.3.5",
  "description": "A lightweight commandline tool for visual regression testing of UI components.",
  "main": "bin/argus-eyes.js",
  "bin": {
    "argus-eyes": "bin/argus-eyes.js"
  },
  "scripts": {
    "pretest": "node tests/mock-server/server-controller.js start",
    "test": "node node_modules/mocha/bin/mocha --recursive --timeout 0 tests/spec/",
    "posttest": "node tests/mock-server/server-controller.js stop"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=4.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/branneman/argus-eyes.git"
  },
  "keywords": [
    "visual regression",
    "visual",
    "regression",
    "testing",
    "phantomjs",
    "phantomcss",
    "imagemagick"
  ],
  "author": "Bran van der Meer <branmovic@gmail.com> (http://bran.name/)",
  "contributors": [
    "Maarten Groeneweg <mgroeneweg@mirabeau.nl>",
    "Marius de Corte <mdecorte@gmail.com> (http://mariusdecorte.nl)"
  ],
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/branneman/argus-eyes/issues"
  },
  "homepage": "https://github.com/branneman/argus-eyes#readme",
  "dependencies": {
    "chalk": "^1.1.1",
    "glob": "^6.0.4",
    "mkdirp": "^0.5.1",
    "phantomjs": "^1.9.19",
    "rimraf": "^2.5.0",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "express": "^4.13.3",
    "mocha": "^2.3.4"
  }
}
